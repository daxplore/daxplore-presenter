<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:org.daxplore.presenter.admin.view">
	
	<g:VerticalPanel>
		<g:Label ui:field='uploadHeader'></g:Label>
		<g:FormPanel ui:field='uploadForm'>
			<g:VerticalPanel>
				<g:Label>Upload a file:</g:Label>
				<g:FileUpload ui:field='uploadWidget' name='dataUpload'/>
				<g:Button ui:field='uploadButton'>Submit</g:Button>
				<g:TextArea ui:field='serverMessageArea'/>
			</g:VerticalPanel>		
		</g:FormPanel>
	</g:VerticalPanel>
</ui:UiBinder> 
