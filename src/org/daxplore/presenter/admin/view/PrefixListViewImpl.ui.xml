<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:com.google.gwt.user.cellview.client">
	
	<g:DecoratorPanel>
		<g:VerticalPanel>
			<g:Label>Select a prefix</g:Label>
			<w:CellList ui:field='prefixCellList'/>
			<g:Button ui:field='addPrefixButton'>Add new prefix</g:Button>
		</g:VerticalPanel>
	</g:DecoratorPanel>
</ui:UiBinder> 
