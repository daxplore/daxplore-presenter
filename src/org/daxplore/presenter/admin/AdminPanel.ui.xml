<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
		
	<g:FormPanel ui:field='uploadForm'>
		<g:VerticalPanel>
			<g:Label>Upload a file:</g:Label>
			<g:FileUpload ui:field='uploadWidget' name='dataUpload'/>
			<g:Button ui:field='uploadButton'>Submit</g:Button>
		</g:VerticalPanel>		
	</g:FormPanel>
	
</ui:UiBinder> 