<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.2.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/2.2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='embed'>
	<!-- Add Google Analytics support -->
	<script src="/js/ga.js" />
	
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />

	<!-- Other modules to inherit -->
	<!-- Added by Daniel on 19 July 2010 -->
	<inherits name='com.googlecode.gchart.GChart' />

	<!-- Added by Daniel on 28 July 2010 -->
	<inherits name="com.google.gwt.http.HTTP" />

	<!-- Added by Axel on 14 October 2010 -->
	<inherits name="com.google.gwt.i18n.I18N" />

	<!-- Added by Axel on 15 October 2010 -->
	<inherits name="com.google.gwt.resources.Resources" />
	<inherits name="com.google.gwt.inject.Inject" />
	
	<!-- Specify the app entry point class. -->
	<entry-point class="org.daxplore.presenter.embed.EmbedEntryPoint" />

	<!-- Specify locales -->
	<extend-property name="locale" values="en,sv"/>
	<set-property-fallback name="locale" value="sv"/>
	<set-property name="locale" value="en,sv"/> 

	<!-- Specify the paths for translatable code -->
	<source path='chart' />
	<source path='embed' />
	<source path='client/json/shared' />
	<source path='shared' />
</module>